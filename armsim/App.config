<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <system.diagnostics>
    <trace autoflush="true" indentsize="0">
      <listeners>
        <add name="MyLog" type="System.Diagnostics.TextWriterTraceListener" initializeData="armSimLogFile.txt" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>
